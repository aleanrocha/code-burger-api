{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["standard", "eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "camelcase": "off",
    "prettier/prettier": "error"
  }
}